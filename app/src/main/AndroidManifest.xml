<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.wordworld">

    <application
            android:allowBackup="true"
            android:label="WordWorld"
            android:icon="@mipmap/app_icon"
            android:roundIcon="@mipmap/app_icon"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">

    <activity android:name=".MainActivity"
              android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity android:name=".LevelOneActivity" />
        <activity android:name=".LevelTwoActivity" />
        <activity android:name=".LevelThreeActivity" />
        <activity android:name=".LoginActivity" android:exported="true"/>
        <activity android:name=".SignUpActivity" android:exported="true" />
        <activity android:name=".ProfileActivity" android:exported="true" />
</application>

        </manifest>
